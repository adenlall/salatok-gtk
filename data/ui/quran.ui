<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<template class="QuranWidget">
          <property name="layout-manager">
		          <object class="GtkBoxLayout">
                 <property name="orientation">vertical</property>
		          </object>
          </property>
          <child>
              <object class="GtkBox">
                 <property name="orientation">vertical</property>
                 <property name="valign">0</property>
                 <property name="halign">0</property>
                 <property name="margin-start">10</property>
                 <property name="margin-end">10</property>
                 <property name="margin-bottom">10</property>
                 <property name="margin-top">5</property>
                  <child>
                  <object class="GtkBox">
                     <property name="valign">center</property>
                     <property name="halign">center</property>
                     <property name="margin-start">5</property>
                     <property name="margin-end">5</property>
                     <property name="spacing">10</property>
                      <property name="hexpand">1</property>
                              <child>

                                <object class="GtkBox">
                                      <property name="spacing">10</property>
                                      <property name="hexpand">1</property>
                                      <property name="halign">start</property>
                                      <property name="orientation">vertical</property>
                                      <child>
                                        <object class="GtkBox">
                                          <property name="halign">start</property>
                                          <property name="hexpand">1</property>
                                              <property name="spacing">10</property>
                                          <child>
                                            <object class="GtkBox">
                                              <property name="halign">start</property>
                                              <property name="spacing">10</property>
                                              <child>
                                                <object class="GtkButton" id="qprev">
                                                 <property name="icon-name">go-previous-symbolic</property>
                                                </object>
                                              </child>
                                              <child>
                                                <object class="GtkButton" id="qnext">
                                                 <property name="icon-name">go-next-symbolic</property>
                                                </object>
                                              </child>
                                            </object>
                                          </child>

                                          <child>
                                            <object class="GtkComboBoxText" id="qcombo">
                                            </object>
                                          </child>

                                          <child>
                                            <object class="GtkComboBoxText" id="qlanguage">
                                            </object>
                                          </child>

                                        </object>
                                      </child>

                                    </object>
                              </child>


                    <child>
                      <object class="GtkMenuButton">
                        <property name="focusable">1</property>
                        <property name="focus-on-click">1</property>
                        <property name="halign">end</property>
                        <property name="valign">center</property>
                        <property name="popover">
                            <object class="GtkPopover">
                              <child>

				                        <object class="GtkScrolledWindow">
                                  <property name="hscrollbar-policy">never</property>
                                  <property name="min-content-height">200</property>
                                  <property name="hexpand">0</property>
                                  <property name="vexpand">1</property>
                                  <child>
                                    <object class="GtkViewport">
                                      <property name="scroll-to-focus">1</property>
                                      <child>
                                        <object class="GtkBox">
                                          <property name="orientation">vertical</property>

                                          <child>
                                            <object class="GtkFrame">
                                              <child>
                                                <object class="GtkListBox">
                                                  <property name="selection-mode">none</property>
                                                  <property name="show-separators">1</property>
                                                  <style>
                                                    <class name="rich-list"/>
                                                  </style>



                                                  <child>
                                                    <object class="GtkListBoxRow">
                                                    <property name="activatable">0</property>
                                                      <child>
                                                        <object class="GtkBox">
                                                          <child>
                                                            <object class="GtkLabel">
                                                              <property name="label" translatable="yes">let text selectable</property>
                                                              <property name="xalign">0</property>
                                                              <property name="halign">start</property>
                                                              <property name="valign">center</property>
                                                              <property name="hexpand">1</property>
                                                            </object>
                                                          </child>

                                                          <child>
                                                            <object class="GtkCheckButton" id="qselectable">
                                                              <property name="halign">end</property>
                                                              <property name="valign">center</property>
                                                            </object>
                                                          </child>

                                                        </object>
                                                      </child>
                                                    </object>
                                                  </child>



                                                  <child>
                                                    <object class="GtkListBoxRow">
                                                    <property name="activatable">0</property>
                                                      <child>
                                                        <object class="GtkBox">
                                                          <child>
                                                            <object class="GtkLabel">
                                                              <property name="label" translatable="yes">Arabic Font Type</property>
                                                              <property name="xalign">0</property>
                                                              <property name="halign">start</property>
                                                              <property name="valign">center</property>
                                                              <property name="hexpand">1</property>
                                                            </object>
                                                          </child>
                                                          <child>
                                                            <object class="GtkComboBoxText" id="fonttype">
                                                               <items>
                                                                    <item id="2">Hafs</item>
                                                                    <item id="3">Warsh</item>
                                                                    <item id="4">KSA</item>
                                                                    <item id="1">Kufi</item>
                                                                </items>
                                                            </object>
                                                          </child>
                                                        </object>
                                                      </child>
                                                    </object>
                                                  </child>




                                                  <child>
                                                    <object class="GtkListBoxRow">
                                                    <property name="activatable">0</property>
                                                      <child>
                                                        <object class="GtkBox">
                                                          <child>
                                                            <object class="GtkLabel">
                                                              <property name="label" translatable="yes">Arabic Font Type</property>
                                                              <property name="xalign">0</property>
                                                              <property name="halign">start</property>
                                                              <property name="valign">center</property>
                                                              <property name="hexpand">1</property>
                                                            </object>
                                                          </child>
                                                          <child>
                                                             <object class="GtkScale" id="fontsize">
                                                               <property name="width-request">180</property>
                                                               <property name="round-digits">0</property>
                                                               <property name="draw-value">0</property>
                                                               <property name="value-pos">2</property>
                                                               <property name="adjustment">
                                                                  <object class="GtkAdjustment">
                                                                    <property name="upper">30</property>
                                                                    <property name="lower">10</property>
                                                                    <property name="value">20</property>
                                                                    <property name="step-increment">1</property>
                                                                    <property name="page-increment">2</property>
                                                                  </object>
                                                               </property>
                                                               <layout>
                                                                 <property name="column">1</property>
                                                                 <property name="row">2</property>
                                                               </layout>
                                                             </object>
                                                          </child>
                                                        </object>
                                                      </child>
                                                    </object>
                                                  </child>
                                                </object>
                                              </child>
                                            </object>
                                          </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                </object>
                            </child>
                          </object>
                        </property>
                      </object>
                    </child>





                  </object>
                </child>

                  <child>
                    <object class="GtkScrolledWindow">
                      <property name="hscrollbar-policy">3</property>
                      <property name="hexpand">1</property>
                      <property name="vexpand">1</property>
                      <property name="margin-top">10</property>
                      <child>
                        <object class="GtkLabel" id="quran">
                          <property name="wrap">1</property>
                          <property name="vexpand">1</property>
                          <property name="hexpand">1</property>
                          <property name="justify">3</property>
                          <property name="selectable">0</property>
                          <property name="label">Loading...</property>
                        </object>
                      </child>
                    </object>
                  </child>

            </object>
          </child>
</template>
</interface>

